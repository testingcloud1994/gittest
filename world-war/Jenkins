pipeline {
    agent any

    stages {
        stage('Init') {
            steps {
                echo 'This is Devops project'
                echo 'This will continue to develop'
            }
        }
        stage('Build') {
            steps {
                echo 'Building project and its artifacts '
            }
        }
        stage('Test') {
            steps {
                echo 'Deploying in testing enviromet ..'
            }
        }
        stage('Prod') {
            steps {
                echo 'Deploying in production enviroment ..'
            }
        }
    }
}